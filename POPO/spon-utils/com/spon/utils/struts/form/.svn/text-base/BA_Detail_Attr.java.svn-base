package com.spon.utils.struts.form;

import java.util.List;

public class BA_Detail_Attr {
	
	
	private int WIDTH=100;
	private String SQL="",SQLOTHER="";
	private String CFIELD_NAME="";
	private String SQLKEYID="";
	private String DG_KEYVALUE_CNAME="";
	private String LOVFIELD_NAME="";
	private String LOVFIELD_ID="";
	private String TH_ID="";
	private String TH_NAME="";
	private String VALUE="";
	
	/*
	 * DISP_TYPE之定義方式
	 * 
	 *	1	2	3
	 *  S	K	R
	 *  T	N	E
	 *  L		H
	 *  
	 *  1.代表型態 S:select T:texe L:lov
	 *  2.代表是否為Key
	 *  3.R 唯讀 E 可編輯  H 隱藏 
	 */
	
	private String DISP_TYPE="TNE";
	private int MAXLENGTH=100;
	private List OPTIONS;
	private boolean isRequired=false;
	
	public List getOPTIONS() {
		return OPTIONS;
	}
	public void setOPTIONS(List options) {
		OPTIONS = options;
	}
	public String getTH_ID() {
		return TH_ID;
	}
	public String getTH_NAME() {
		return TH_NAME;
	}
	public int getWIDTH() {
		return WIDTH;
	}
	
	public void setTH_ID(String th_id) {
		TH_ID = th_id;
	}
	public void setTH_NAME(String th_name) {
		TH_NAME = th_name;
	}
	/**
	 * 設定欄位寬度  預設值 為100
	 * @param width
	 */
	public void setWIDTH(int width) {
		WIDTH = width;
	}
	public String getDISP_TYPE() {
		return DISP_TYPE;
	}
	public void setDISP_TYPE(String disp_type) {
		DISP_TYPE = disp_type;
	}
	public String getVALUE() {
		return VALUE;
	}
	public void setVALUE(String value) {
		VALUE = value;
	}
	public int getMAXLENGTH() {
		return MAXLENGTH;
	}
	public void setMAXLENGTH(int maxlength) {
		MAXLENGTH = maxlength;
	}
	public String getCFIELD_NAME() {
		return CFIELD_NAME;
	}
	public String getDG_KEYVALUE_CNAME() {
		return DG_KEYVALUE_CNAME;
	}
	public String getSQL() {
		return SQL;
	}
	public String getSQLKEYID() {
		return SQLKEYID;
	}
	public void setCFIELD_NAME(String cfield_name) {
		CFIELD_NAME = cfield_name;
	}
	public void setDG_KEYVALUE_CNAME(String dg_keyvalue_cname) {
		DG_KEYVALUE_CNAME = dg_keyvalue_cname;
	}
	public void setSQL(String sql) {
		SQL = sql;
	}
	public void setSQLKEYID(String sqlkeyid) {
		SQLKEYID = sqlkeyid;
	}
	public boolean isRequired() {
		return isRequired;
	}
	public void setRequired(boolean isRequired) {
		this.isRequired = isRequired;
	}
	public String getLOVFIELD_NAME() {
		return LOVFIELD_NAME;
	}
	public void setLOVFIELD_NAME(String lovfield_name) {
		LOVFIELD_NAME = lovfield_name;
	}
	public String getLOVFIELD_ID() {
		return LOVFIELD_ID;
	}
	public void setLOVFIELD_ID(String lovfield_id) {
		LOVFIELD_ID = lovfield_id;
	}
	public String getSQLOTHER() {
		return SQLOTHER;
	}
	public void setSQLOTHER(String sqlother) {
		SQLOTHER = sqlother;
	}
	
	
}
